[reviewboard]
    url = https://scm.tagged.com/reviewboard
[user]
    name = Nate Kresge
    email = nkresge@tagged.com
[color]
    ui = auto
[color "branch"]
    current = blue reverse
    local = blue
    remote = green
[color "diff"]
    meta = blue
    frag = magenta
    old = red
    new = green
    whitespace = red reverse
[color "status"]
    added = blue
    changed = green
    untracked = cyan
[core]
    ignorecase=true
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
    excludesfile="/Users/nkresge/git/common-exclusions"
    editor=mvim -v
[log]
    date=local
[alias]
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    st = status
    ci = commit
    di = diff
    co = checkout 
    rb = !post-review --parent=trunk --guess-description --target-groups=games --username=nkresge -o
    rb-diff = !post-review -n --parent=trunk
    sdiff = !git-svn-diff
